-- ========================================
-- SERVIÇOS / SERVICES
-- ========================================

local TweenService = game:GetService("TweenService")
local UserInputService = game:GetService("UserInputService")
local Players = game:GetService("Players")
local ReplicatedStorage = game:GetService("ReplicatedStorage")

-- ========================================
-- VARIÁVEIS / VARIABLES
-- ========================================

local LocalPlayer = Players.LocalPlayer

-- Variáveis de tracking de ganho
local initialStrength = 0
local lastStrength = 0
local strengthPerSecond = 0
local spsUpdateTime = 0

-- Variáveis de tempo de farm
local farmStartTime = nil
local farmElapsedTime = 0
local isFarmingTime = false

-- Variáveis de Farm (Ghost Hub)
_G.Equip = false
_G.farmEnabled = false
_G.farmActive = false
_G.pingLimiterEnabled = true -- Sempre ativo com 23.5k
_G.autoFarmAfterDupe = false
_G.customPingLimit = 23500 -- 23.5k fixo
_G.resumePingThreshold = 3000

local isPingStopped = false

-- ========================================
-- FUNÇÕES AUXILIARES / HELPER FUNCTIONS
-- ========================================

-- Função para formatar números grandes (exemplo: 3988900600 → "3.98b")
local function formatNumber(num)
	if num >= 1e12 then
		return string.format("%.2ft", math.floor(num / 1e10) / 100)
	elseif num >= 1e9 then
		return string.format("%.2fb", math.floor(num / 1e7) / 100)
	elseif num >= 1e6 then
		return string.format("%.2fm", math.floor(num / 1e4) / 100)
	elseif num >= 1e3 then
		return string.format("%.2fk", math.floor(num / 10) / 100)
	else
		return tostring(math.floor(num))
	end
end

local function formatFarmTime(totalSeconds)
	local days = math.floor(totalSeconds / 86400)
	local hours = math.floor((totalSeconds % 86400) / 3600)
	local minutes = math.floor((totalSeconds % 3600) / 60)
	local seconds = totalSeconds % 60
	
	return string.format("%dd, %dh, %dm, %ds", days, hours, minutes, seconds)
end

-- Sistema de contagem de tempo de farm (continua mesmo com ping limiter pausado)
spawn(function()
	while wait(1) do
		if isFarmingTime then
			farmElapsedTime = farmElapsedTime + 1
			TimeFarming.Text = "Time Farming: " .. formatFarmTime(farmElapsedTime)
		end
	end
end)

-- Verificação de 2 minutos (re-equipa e retoma farm se bugar)
spawn(function()
	while wait(120) do -- A cada 2 minutos
		if _G.farmActive then
			pcall(function()
				local hasEquippedWeights = false
				local currentStrength = 0
				
				-- Verifica se tem weights equipadas
				if LocalPlayer.Character then
					for _, item in pairs(LocalPlayer.Character:GetChildren()) do
						if item.Name == "Double Weight" then
							hasEquippedWeights = true
							break
						end
					end
				end
				
				-- Se não tem weights equipadas mas farm está ativo, re-equipa
				if not hasEquippedWeights and _G.farmActive then
					autoEquipWeights()
					wait(1)
				end
				
				-- NOVO: Verifica se o farm parou mesmo com weights equipados e retoma
				if hasEquippedWeights and not _G.farmEnabled then
					-- Farm parou mas tem weights - retoma o farm
					_G.farmEnabled = true
					_G.farmActive = true
					
					-- Garante que a contagem de tempo continue
					if not isFarmingTime then
						isFarmingTime = true
					end
					if not farmStartTime then
						farmStartTime = os.time()
					end
					
					farmLoop()
				end
			end)
		end
	end
end)
local function abbreviatePing(ping)
	if ping >= 1000 then
		return string.format("%.1fk", ping / 1000)
	else
		return tostring(math.floor(ping))
	end
end

local function formatWaitTime(waitTime)
	return string.format("%.6f", waitTime)
end

local function getCurrentPing()
	local Stats = game:GetService("Stats")
	local pingValue = Stats.Network.ServerStatsItem["Data Ping"]
	if pingValue then
		return tonumber(pingValue:GetValue()) or 0
	end
	return 0
end

local function countAllWeights()
	local count = 0
	pcall(function()
		for _, item in pairs(LocalPlayer.Backpack:GetChildren()) do
			if item.Name == "Double Weight" then 
				count = count + 1 
			end
		end
		if LocalPlayer.Character then
			for _, item in pairs(LocalPlayer.Character:GetChildren()) do
				if item.Name == "Double Weight" then 
					count = count + 1 
				end
			end
		end
	end)
	return count
end

local function calculateWaitTime()
	local weightCount = countAllWeights()
	return 0.00065 * weightCount
end

local function checkPingLimit()
	if not _G.pingLimiterEnabled then 
		isPingStopped = false
		return true 
	end
	
	local currentPing = getCurrentPing()
	
	if currentPing >= _G.customPingLimit and not isPingStopped then
		isPingStopped = true
		return false
	elseif currentPing < _G.resumePingThreshold and isPingStopped then
		isPingStopped = false
		return true
	end
	
	return not isPingStopped
end

local function autoEquipWeights()
	local equipped = 0
	local success = pcall(function()
		local batchCount = 0
		for _, item in pairs(LocalPlayer.Backpack:GetChildren()) do
			if item.Name == "Double Weight" then
				item.Parent = LocalPlayer.Character
				if item:FindFirstChild("Handle") then
					item.Handle.CanCollide = false
					item.Handle.Massless = true
				end
				equipped = equipped + 1
				batchCount = batchCount + 1
				
				if batchCount >= 100 then
					wait(0.05)
					batchCount = 0
				end
			end
		end
	end)
	
	wait(0.5)
	return success and equipped or 0
end

local function farmLoop()
	local errorCount = 0
	local maxErrors = 10
	
	spawn(function()
		while _G.farmEnabled do
			local success = pcall(function()
				if not LocalPlayer.Character then
					wait(2)
					return
				end
				
				if not LocalPlayer.Parent then
					_G.farmEnabled = false
					_G.farmActive = false
					return
				end
				
				if checkPingLimit() then
					local waitTime = calculateWaitTime()
					
					if waitTime and waitTime > 0 and waitTime < 60 then
						task.wait(waitTime)
					else
						task.wait(0.001)
					end
					
					if LocalPlayer.Character then
						for _, item in pairs(LocalPlayer.Character:GetChildren()) do
							if item.Name == "Double Weight" then
								item:Activate()
							end
						end
					end
					
					errorCount = 0
				else
					task.wait(1.5)
				end
			end)
			
			if not success then
				errorCount = errorCount + 1
				if errorCount >= maxErrors then
					_G.farmEnabled = false
					_G.farmActive = false
					break
				end
				wait(2)
			end
		end
		
		_G.farmActive = false
	end)
end

-- Função para atualizar o label "Strenght Total" com dados do ReplicatedStorage
local function updateStrenghtTotal()
	local success, err = pcall(function()
		-- Tentar ReplicatedStorage primeiro
		local dataFolder = ReplicatedStorage:FindFirstChild("Data")
		if dataFolder then
			local playerFolder = dataFolder:FindFirstChild(LocalPlayer.Name)
			if playerFolder then
				local strengthValue = playerFolder:FindFirstChild("Strenght") or playerFolder:FindFirstChild("Strength")
				if strengthValue and (strengthValue:IsA("IntValue") or strengthValue:IsA("NumberValue")) then
					local value = strengthValue.Value
					local formatted = formatNumber(value)
					StrenghtTotal.Text = string.format("Strenght Total: %s  (%d)", formatted, value)
					return
				end
			end
		end
		
		-- Se não encontrar em ReplicatedStorage, tentar pegar do leaderstats mesmo
		local leaderstats = LocalPlayer:FindFirstChild("leaderstats")
		if leaderstats then
			local strengthValue = leaderstats:FindFirstChild("Strenght") or leaderstats:FindFirstChild("Strength")
			if strengthValue and (strengthValue:IsA("IntValue") or strengthValue:IsA("NumberValue")) then
				local value = strengthValue.Value
				local formatted = formatNumber(value)
				StrenghtTotal.Text = string.format("Strenght Total: %s  (%d)", formatted, value)
			end
		end
	end)
	
	if not success then
		warn("Erro ao atualizar Strenght Total: " .. tostring(err))
	end
end

-- Função para atualizar o label "Size" com dados do leaderstats
local function updateSize()
	local success, err = pcall(function()
		local leaderstats = LocalPlayer:FindFirstChild("leaderstats")
		if leaderstats then
			local strengthValue = leaderstats:FindFirstChild("Strenght") or leaderstats:FindFirstChild("Strength")
			if strengthValue and (strengthValue:IsA("IntValue") or strengthValue:IsA("NumberValue")) then
				local value = strengthValue.Value
				local formatted = formatNumber(value)
				Size.Text = string.format("Size: %s", formatted)
			end
		end
	end)
	
	if not success then
		warn("Erro ao atualizar Size: " .. tostring(err))
	end
end

-- Função para atualizar o label "Strenght Gained"
local function updateStrenghtGained()
	local success, err = pcall(function()
		local leaderstats = LocalPlayer:FindFirstChild("leaderstats")
		if leaderstats then
			local strengthValue = leaderstats:FindFirstChild("Strenght") or leaderstats:FindFirstChild("Strength")
			if strengthValue and (strengthValue:IsA("IntValue") or strengthValue:IsA("NumberValue")) then
				local currentStrength = strengthValue.Value
				
				-- Se é a primeira vez, definir o valor inicial
				if initialStrength == 0 then
					initialStrength = currentStrength
				end
				
				local gained = currentStrength - initialStrength
				local formatted = formatNumber(gained)
				StrenghtGained.Text = string.format("Strenght Gained: +%s", formatted)
			end
		end
	end)
	
	if not success then
		warn("Erro ao atualizar Strenght Gained: " .. tostring(err))
	end
end

-- Função para calcular e atualizar SPS (Strength Per Second)
local function updateSps()
	local success, err = pcall(function()
		local leaderstats = LocalPlayer:FindFirstChild("leaderstats")
		if leaderstats then
			local strengthValue = leaderstats:FindFirstChild("Strenght") or leaderstats:FindFirstChild("Strength")
			if strengthValue and (strengthValue:IsA("IntValue") or strengthValue:IsA("NumberValue")) then
				local currentStrength = strengthValue.Value
				local currentTime = tick()
				
				-- Inicializar na primeira vez
				if lastStrength == 0 then
					lastStrength = currentStrength
					spsUpdateTime = currentTime
					Sps.Text = "Sps: 0 (+0)"
					return
				end
				
				-- Calcular SPS baseado na diferença desde a última atualização
				if spsUpdateTime > 0 then
					local timeDiff = currentTime - spsUpdateTime
					if timeDiff >= 1 then -- Atualizar a cada 1 segundo
						local strengthDiff = currentStrength - lastStrength
						strengthPerSecond = math.floor(strengthDiff / timeDiff)
						
						local formatted = formatNumber(strengthPerSecond)
						Sps.Text = string.format("Sps: %s (+%d)", formatted, strengthPerSecond)
						
						-- Atualizar para próximo cálculo
						lastStrength = currentStrength
						spsUpdateTime = currentTime
					end
				end
			end
		end
	end)
	
	if not success then
		warn("Erro ao atualizar SPS: " .. tostring(err))
	end
end

-- ========================================
-- FUNÇÕES / FUNCTIONS
-- ========================================

-- Função para alternar entre as abas (frames) SEM animação
local function showFrame(frameToShow, selectedButton)
	-- Esconder todos os frames instantaneamente
	for _, frame in pairs({Frame1, Frame2, Frame3, Frame4, Frame5}) do
		frame.Visible = false
	end
	
	-- Resetar cor de todos os botões para branco (não selecionado)
	for _, button in pairs({ButtonFrame1, ButtonFrame2, ButtonFrame3, ButtonFrame4, ButtonFrame5}) do
		TweenService:Create(button, TweenInfo.new(0.2, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {
			BackgroundColor3 = Color3.fromRGB(255, 255, 255),
			BackgroundTransparency = 1.000
		}):Play()
	end
	
	-- Destacar o botão selecionado com cor azul e animação
	TweenService:Create(selectedButton, TweenInfo.new(0.2, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {
		BackgroundColor3 = Color3.fromRGB(43, 0, 255),
		BackgroundTransparency = 0.300
	}):Play()
	
	-- Mostrar o frame selecionado instantaneamente
	frameToShow.Visible = true
end

-- Botão Close - Fecha a GUI com animação
Close.MouseButton1Click:Connect(function()
	-- Animação de fechar (escala para 0)
	TweenService:Create(Frame, TweenInfo.new(0.3, Enum.EasingStyle.Back, Enum.EasingDirection.In), {
		Size = UDim2.new(0, 0, 0, 0),
		Position = UDim2.new(0.5, 0, 0.5, 0)
	}):Play()
	
	wait(0.3)
	ScreenGui:Destroy()
end)

-- Botões das Abas
ButtonFrame1.MouseButton1Click:Connect(function()
	showFrame(Frame1, ButtonFrame1)
end)

ButtonFrame2.MouseButton1Click:Connect(function()
	showFrame(Frame2, ButtonFrame2)
end)

ButtonFrame3.MouseButton1Click:Connect(function()
	showFrame(Frame3, ButtonFrame3)
end)

ButtonFrame4.MouseButton1Click:Connect(function()
	showFrame(Frame4, ButtonFrame4)
	-- Atualizar os valores quando abrir Frame4
	updateStrenghtTotal()
	updateSize()
	updateStrenghtGained()
	updateSps()
end)

ButtonFrame5.MouseButton1Click:Connect(function()
	showFrame(Frame5, ButtonFrame5)
end)

-- ========================================
-- BOTÕES DE FARM (Frame1)
-- ========================================

-- Atualizar Wait Time e Ping Monitor
spawn(function()
	while wait(1) do
		pcall(function()
			local waitTime = calculateWaitTime()
			-- Concatena direto, SEM formatação (igual Zaza Hub)
			WaitTime.Text = "Wait Time: " .. waitTime
		end)
	end
end)

-- Botão Equip
Equip.MouseButton1Click:Connect(function()
	_G.Equip = not _G.Equip
	if _G.Equip then
		spawn(function()
			pcall(function()
				local batchCount = 0
				for _, item in pairs(LocalPlayer.Backpack:GetChildren()) do
					if item.Name == "Double Weight" then
						item.Parent = LocalPlayer.Character
						if item:FindFirstChild("Handle") then
							item.Handle.CanCollide = false
							item.Handle.Massless = true
						end
						batchCount = batchCount + 1
						
						if batchCount >= 100 then
							wait(0.05)
							batchCount = 0
						end
					end
				end
			end)
		end)
	else
		spawn(function()
			pcall(function()
				local batchCount = 0
				if LocalPlayer.Character then
					for _, item in pairs(LocalPlayer.Character:GetChildren()) do
						if item.Name == "Double Weight" then
							item.Parent = LocalPlayer.Backpack
							batchCount = batchCount + 1
							
							if batchCount >= 100 then
								wait(0.05)
								batchCount = 0
							end
						end
					end
				end
			end)
		end)
	end
end)

-- Botão Farm
Farm.MouseButton1Click:Connect(function()
	if not _G.farmActive then
		local hasEquippedWeights = false
		pcall(function()
			if LocalPlayer.Character then
				for _, item in pairs(LocalPlayer.Character:GetChildren()) do
					if item.Name == "Double Weight" then
						hasEquippedWeights = true
						break
					end
				end
			end
		end)
		
		if not hasEquippedWeights then
			return
		end
		
		_G.farmEnabled = true
		_G.farmActive = true
		
		-- Inicia contagem de tempo
		isFarmingTime = true
		if not farmStartTime then
			farmStartTime = os.time()
			farmElapsedTime = 0
		end
		
		farmLoop()
	end
end)

-- Botão Stop Equip
StopEquip.MouseButton1Click:Connect(function()
	_G.Equip = false
	spawn(function()
		pcall(function()
			local batchCount = 0
			if LocalPlayer.Character then
				for _, item in pairs(LocalPlayer.Character:GetChildren()) do
					if item.Name == "Double Weight" then
						item.Parent = LocalPlayer.Backpack
						batchCount = batchCount + 1
						
						if batchCount >= 100 then
							wait(0.05)
							batchCount = 0
						end
					end
				end
			end
		end)
	end)
end)

