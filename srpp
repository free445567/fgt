-- ========================================
-- BOTÕES DE DUPE (Frame2)
-- ========================================

-- Função para contar Double Weights
local function countAllWeights()
	local count = 0
	pcall(function()
		local backpack = LocalPlayer:FindFirstChild("Backpack")
		if backpack then
			for _, tool in pairs(backpack:GetChildren()) do
				if tool:IsA("Tool") and tool.Name == "Double Weight" then
					count = count + 1
				end
			end
		end
		
		local character = LocalPlayer.Character
		if character then
			for _, tool in pairs(character:GetChildren()) do
				if tool:IsA("Tool") and tool.Name == "Double Weight" then
					count = count + 1
				end
			end
		end
	end)
	return count
end

-- Função para contar Double Weights APENAS no Backpack (não equipados)
local function countBackpackWeights()
	local count = 0
	pcall(function()
		local backpack = LocalPlayer:FindFirstChild("Backpack")
		if backpack then
			for _, tool in pairs(backpack:GetChildren()) do
				if tool:IsA("Tool") and tool.Name == "Double Weight" then
					count = count + 1
				end
			end
		end
	end)
	return count
end

-- Atualizar contador de Double Weights
spawn(function()
	while wait(0.5) do
		pcall(function()
			local backpackCount = countBackpackWeights()
			local totalCount = countAllWeights()
			
			-- Se não tem nada no backpack mas tem equipado, mostra o total
			if backpackCount == 0 and totalCount > 0 then
				CounterDw.Text = "Double Weight(s): " .. tostring(totalCount)
			else
				CounterDw.Text = "Double Weight(s): " .. tostring(backpackCount)
			end
		end)
	end
end)

-- Variável de controle de dupe
local isDuping = false
local remainingDupes = 0

-- Função de Dupe (EXATAMENTE como no Ghost Hub)
local function startDuping(amount)
	if isDuping then
		return
	end
	isDuping = true
	
	-- Verifica quantos pesos tem AGORA
	local currentWeights = countAllWeights()
	
	-- Se tiver exatamente 1 peso (o inicial), subtrai 1 do valor digitado
	-- Se tiver mais de 1 peso, usa o valor exato (não subtrai nada)
	if currentWeights == 1 then
		remainingDupes = amount - 1  -- Tem só o peso inicial: digita 10, dupa 9, fica com 10
	else
		remainingDupes = amount  -- Já tem pesos: digita 20, dupa 20, adiciona 20
	end
	
	spawn(function()
		local marketplaceService = game:GetService('MarketplaceService')
		for remaining = remainingDupes, 1, -1 do
			if not isDuping then 
				remainingDupes = remaining
				break 
			end
			
			local totalSeconds = remaining * 1
			local minutes = math.floor(totalSeconds / 60)
			local seconds = math.floor(totalSeconds % 60)
			
			-- Formato: "Xm Ys" ou apenas "Xs" se for menos de 1 minuto
			if minutes > 0 then
				TempoDeDupe.Text = string.format("Time: %dm %ds", minutes, seconds)
			else
				TempoDeDupe.Text = string.format("Time: %ds", seconds)
			end
			
			pcall(function()
				marketplaceService:SignalPromptGamePassPurchaseFinished(LocalPlayer, 5949054, true)
			end)
			wait(1)
		end
		
		if isDuping then
			TempoDeDupe.Text = "Time: 0s"
			remainingDupes = 0
			
			-- Auto Farm After Dupe
			if _G.autoFarmAfterDupe then
				wait(2)
				
				local equippedCount = autoEquipWeights()
				wait(0.5)
				
				if equippedCount > 0 then
					wait(1)
					
					if not _G.farmActive then
						_G.farmEnabled = true
						_G.farmActive = true
						
						-- Inicia contagem de tempo quando auto farm começar
						isFarmingTime = true
						if not farmStartTime then
							farmStartTime = os.time()
							farmElapsedTime = 0
						end
						
						farmLoop()
					end
				end
			end
		else
			local totalSeconds = remainingDupes * 1
			local minutes = math.floor(totalSeconds / 60)
			local seconds = math.floor(totalSeconds % 60)
			if minutes > 0 then
				TempoDeDupe.Text = string.format("Time: %dm %ds - %d left", minutes, seconds, remainingDupes)
			else
				TempoDeDupe.Text = string.format("Time: %ds - %d left", seconds, remainingDupes)
			end
		end
		isDuping = false
	end)
end

-- Botão Dupe
BotoDupe.MouseButton1Click:Connect(function()
	local amount = tonumber(TextBox.Text)
	if amount and amount > 0 then
		startDuping(amount)
	else
		TextBox.Text = "Invalid Number!"
		wait(1)
		TextBox.Text = ""
	end
end)

-- ========================================
-- SISTEMA DE ARRASTAR GUI (DRAG) COM SMOOTH
-- ========================================

local dragging
local dragInput
local dragStart
local startPos

local function update(input)
	local delta = input.Position - dragStart
	local targetPos = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)
	
	-- Animação suave ao arrastar
	TweenService:Create(Frame, TweenInfo.new(0.1, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {
		Position = targetPos
	}):Play()
end

Tittle.InputBegan:Connect(function(input)
	if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
		dragging = true
		dragStart = input.Position
		startPos = Frame.Position
		
		input.Changed:Connect(function()
			if input.UserInputState == Enum.UserInputState.End then
				dragging = false
			end
		end)
	end
end)

Tittle.InputChanged:Connect(function(input)
	if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
		dragInput = input
	end
end)

UserInputService.InputChanged:Connect(function(input)
	if input == dragInput and dragging then
		update(input)
	end
end)

-- ========================================
-- ANIMAÇÃO DE ABERTURA DA GUI
-- ========================================

-- Começar com a GUI pequena e invisível
Frame.Size = UDim2.new(0, 0, 0, 0)
Frame.Position = UDim2.new(0.5, 0, 0.5, 0)

wait(0.1)

-- Animação de abertura suave (scale up com bounce)
TweenService:Create(Frame, TweenInfo.new(0.5, Enum.EasingStyle.Back, Enum.EasingDirection.Out), {
	Size = UDim2.new(0, 467, 0, 324),
	Position = UDim2.new(0.121900827, 0, 0.312643677, 0)
}):Play()

-- Inicializar com a primeira aba selecionada após animação
wait(0.5)
showFrame(Frame1, ButtonFrame1)

-- ========================================
-- ATUALIZAÇÃO AUTOMÁTICA DOS VALORES
-- ========================================

-- Atualizar valores inicialmente
updateStrenghtTotal()
updateSize()
updateStrenghtGained()
updateSps()

-- Atualizar valores a cada 0.5 segundos
spawn(function()
	while wait(0.5) do
		updateStrenghtTotal()
		updateSize()
		updateStrenghtGained()
		updateSps()
	end
end)

-- Conectar listeners para mudanças nos valores
local function connectValueListeners()
	-- Listener para ReplicatedStorage Strenght
	pcall(function()
		local dataFolder = ReplicatedStorage:FindFirstChild("Data")
		if dataFolder then
			local playerFolder = dataFolder:FindFirstChild(LocalPlayer.Name)
			if playerFolder then
				local strengthValue = playerFolder:FindFirstChild("Strenght") or playerFolder:FindFirstChild("Strength")
				if strengthValue then
					strengthValue.Changed:Connect(updateStrenghtTotal)
				end
			end
		end
	end)
	
	-- Listener para Leaderstats Strenght
	pcall(function()
		local leaderstats = LocalPlayer:FindFirstChild("leaderstats")
		if leaderstats then
			local strengthValue = leaderstats:FindFirstChild("Strenght") or leaderstats:FindFirstChild("Strength")
			if strengthValue then
				strengthValue.Changed:Connect(function()
					updateSize()
					updateStrenghtTotal()
					updateStrenghtGained()
					updateSps()
				end)
			end
		end
	end)
end

connectValueListeners()

print("✨ GUI Zzx carregada com sucesso! - Versão Final com Animações Suaves 🚀")
