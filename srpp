	_G.farmEnabled = false
	task.wait(0.5)
	_G.farmActive = false
	
	-- Para e reseta contagem de tempo
	isFarmingTime = false
	farmStartTime = nil
	farmElapsedTime = 0
	TimeFarming.Text = "Time Farming: 0d, 0h, 0m, 0s"
end)

