-- Dell All + Afk
DellAllAfk.MouseButton1Click:Connect(function()
	-- Remove elementos visuais do jogo
	pcall(function() game.ReplicatedFirst:FindFirstChild("TourneyQ"):Destroy() end)
	pcall(function() game:GetService("Lighting"):FindFirstChild("NormalSky"):Destroy() end)
	pcall(function() LocalPlayer.PlayerGui:FindFirstChild("HUD"):Destroy() end)
	
	-- Camera Zoom
	pcall(function()
		LocalPlayer.CameraMaxZoomDistance = 1000
	end)
	
	-- Função reduceLag otimizada
	local function reduceLag()
		pcall(function()
			local Lighting = game:GetService("Lighting")
			local Terrain = workspace:FindFirstChildOfClass('Terrain')
			
			Terrain.WaterWaveSize = 0
			Terrain.WaterWaveSpeed = 0
			Terrain.WaterReflectance = 0
			Terrain.WaterTransparency = 0
			Lighting.GlobalShadows = false
			Lighting.FogEnd = 9e9
			Lighting.Brightness = 0
			settings().Rendering.QualityLevel = "Level01"
			
			for _, v in pairs(game:GetDescendants()) do
				pcall(function()
					if v:IsA("Part") or v:IsA("Union") or v:IsA("CornerWedgePart") or v:IsA("TrussPart") then
						v.Material = "Plastic"
						v.Reflectance = 0
					elseif v:IsA("Decal") or v:IsA("Texture") then
						v.Transparency = 1
					elseif v:IsA("ParticleEmitter") or v:IsA("Trail") then
						v.Lifetime = NumberRange.new(0)
					elseif v:IsA("Explosion") then
						v.BlastPressure = 1
						v.BlastRadius = 1
					elseif v:IsA("Fire") or v:IsA("SpotLight") or v:IsA("Smoke") or v:IsA("Sparkles") then
						v.Enabled = false
					elseif v:IsA("MeshPart") then
						v.Material = "Plastic"
						v.Reflectance = 0
						v.TextureID = 10385902758728957
					elseif v:IsA("Clouds") then
						v:Destroy()
					end
				end)
			end
			
			for _, e in pairs(Lighting:GetChildren()) do
				pcall(function()
					if e:IsA("BlurEffect") or e:IsA("SunRaysEffect") or e:IsA("ColorCorrectionEffect") 
					or e:IsA("BloomEffect") or e:IsA("DepthOfFieldEffect") then
						e.Enabled = false
					elseif e:IsA("Clouds") then
						e:Destroy()
					end
				end)
			end
			
			-- Remove atmosfera e neblina
			pcall(function()
				if Lighting:FindFirstChildOfClass('Atmosphere') then
					Lighting:FindFirstChildOfClass('Atmosphere'):Destroy()
				end
			end)
		end)
	end
	
	-- Primeira parte: Otimização visual básica
	pcall(function()
		local g = game
		local w = g.Workspace
		local l = g.Lighting
		local t = w.Terrain
		
		t.WaterWaveSize = 0
		t.WaterWaveSpeed = 0
		t.WaterReflectance = 0
		t.WaterTransparency = 0
		l.GlobalShadows = false
		l.FogEnd = 9e9
		l.Brightness = 0
		settings().Rendering.QualityLevel = "Level01"
		
		-- Remove nuvens de forma segura do Workspace
		pcall(function() 
			for _, child in pairs(w:GetChildren()) do
				if child:IsA("Clouds") then
					child:Destroy()
				end
			end
		end)
		
		-- Remove nuvens do Lighting
		pcall(function() 
			for _, cloud in pairs(l:GetChildren()) do
				if cloud:IsA("Clouds") then
					cloud:Destroy()
				end
			end
		end)
		
		-- Remove TODAS as nuvens de TODOS os descendants
		pcall(function()
			for _, obj in pairs(w:GetDescendants()) do
				if obj:IsA("Clouds") then
					obj:Destroy()
				end
			end
		end)
		
		-- Lista de objetos protegidos
		local protectedNames = {
			"Camera", "Water", "Terrain", "CurrentCamera"
		}
		
		local function isProtected(obj)
			for _, name in pairs(protectedNames) do
				if obj.Name == name or obj:IsDescendantOf(workspace.CurrentCamera) then
					return true
				end
			end
			return false
		end
		
		-- Otimização de objetos (evita objetos protegidos)
		for i, v in pairs(w:GetDescendants()) do
			pcall(function()
				if not isProtected(v) then
					if v:IsA("Part") or v:IsA("Union") or v:IsA("CornerWedgePart") or v:IsA("TrussPart") then
						v.Material = "Plastic"
						v.Reflectance = 0
					elseif v:IsA("Decal") or v:IsA("Texture") then
						v.Transparency = 1
					elseif v:IsA("ParticleEmitter") or v:IsA("Trail") then
						v.Lifetime = NumberRange.new(0)
					elseif v:IsA("Explosion") then
						v.BlastPressure = 1
						v.BlastRadius = 1
					elseif v:IsA("Fire") or v:IsA("SpotLight") or v:IsA("Smoke") then
						v.Enabled = false
					elseif v:IsA("MeshPart") then
						v.Material = "Plastic"
						v.Reflectance = 0
						v.TextureID = 10385902758728957
					end
				end
			end)
		end
		
		-- Desativa efeitos de pós-processamento
		for i, e in pairs(l:GetChildren()) do
			pcall(function()
				if e:IsA("BlurEffect") or e:IsA("SunRaysEffect") or e:IsA("ColorCorrectionEffect") 
				or e:IsA("BloomEffect") or e:IsA("DepthOfFieldEffect") then
					e.Enabled = false
				end
			end)
		end
		
		-- Remove atmosfera
		pcall(function()
			if l:FindFirstChildOfClass('Atmosphere') then
				l:FindFirstChildOfClass('Atmosphere'):Destroy()
			end
		end)
		
		-- Remove névoa (Sky)
		pcall(function()
			for _, sky in pairs(l:GetChildren()) do
				if sky:IsA('Sky') then
					sky:Destroy()
				end
			end
		end)
	end)
	
	-- Anti-AFK
	pcall(function()
		local VirtualUser = game:GetService("VirtualUser")
		LocalPlayer.Idled:Connect(function()
			VirtualUser:Button2Down(Vector2.new(0, 0), workspace.CurrentCamera.CFrame)
			task.wait(1)
			VirtualUser:Button2Up(Vector2.new(0, 0), workspace.CurrentCamera.CFrame)
		end)
	end)
	
	-- Desativa UI elements (Chat, PlayerList, Backpack)
	pcall(function()
		local StarterGui = game:GetService("StarterGui")
		StarterGui:SetCoreGuiEnabled(Enum.CoreGuiType.Chat, false)
		StarterGui:SetCoreGuiEnabled(Enum.CoreGuiType.PlayerList, false)
		game:GetService("CoreGui").RobloxGui.Backpack.Visible = false
	end)
	
	-- Chama a função reduceLag otimizada
	reduceLag()
end)

